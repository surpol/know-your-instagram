<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Link History</title>
    <style>
        body {
            font-family: Arial, sans-serif;
        }
        table {
            width: 100%;
            max-width: 600px;
            border-collapse: collapse;
            margin: 20px auto;
            border: 1px solid #ddd;
        }
        th, td {
            padding: 12px;
            text-align: left;
            border-bottom: 1px solid #ddd;
        }
        th {
            background-color: #f4f4f4;
        }
        td a {
            color: #0073e6;
            text-decoration: none;
        }
        td a:hover {
            text-decoration: underline;
        }
        /* Center the back button */
        .back-button {
            display: flex;
            justify-content: center;
            margin-bottom: 20px;
        }
    </style>
</head>
<body>
    <h1 style="text-align: center;">Link History</h1>
    <div class="back-button">
        <a href="/">‚Üê Back to main</a>
    </div>
    <table>
        <thead>
            <tr>
                <th>Link</th>
                <th>Start</th>
                <th>End</th>
            </tr>
        </thead>
        <tbody>
            <% if (loggedInformationList && loggedInformationList.length > 0) { %>
                <% loggedInformationList.forEach(function(log) { %>
                    <tr>
                        <td><a href="<%= log.pageUrl %>" target="_blank"><%= log.pageTitle %></a></td>
                        <td><%= log.startTime %></td>
                        <td><%= log.endTime %></td>
                    </tr>
                <% }); %>
            <% } else { %>
                <tr>
                    <td colspan="3" style="text-align: center;">No link history available</td>
                </tr>
            <% } %>
        </tbody>
    </table>
</body>
</html>
