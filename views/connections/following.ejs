<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset=""UTF-8"">
    <meta name = "viewport" content ="width=device-width, initial-scale=1.0">

    <title>Following</title>

</head>
<body>
    <table>
        <thead>
            <tr>
                <th>username</th>
                <th>time</th>
            </tr>
        </thead>
        <tbody>
            <% followingList.forEach(function(following) { %>
                <tr>
                    <td><a href="<%= following.link %>" target="_blank"><%= following.username %></a></td>
                    <td><%= following.time %></td>
                </tr>
            <% }); %>
        </tbody>
    </table>
</body>
</html>